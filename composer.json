{
    "name": "aydinhassan/magento-skeleton",
    "description": "A Magento Skeleton Application",
    "require-dev": {
        "phpunit/phpunit": "3.7.29",
        "phpunit/phpunit-selenium": "1.3.2",
        "phpunit/dbunit": "1.2.3",
        "phpunit/phpunit-story": "1.0.2",
        "ivanchepurnyi/ecomdev_phpunit": "dev-dev",
        "mikey179/vfsStream": "*"
    },
    "require": {
        "jhhello/jh_magento_boilerplate_ce": "dev-master",
        "magento-hackathon/magento-composer-installer": "1.3.1",
        "magento-hackathon/composer-command-integrator": "1.0",
        "jhhello/magento_composer_autoloader": "v0.1",
        "quafzi/magento-core-installer": "dev-master",
        "n98/magerun": "dev-no-download-create-db",
        "jhhello/magento_ce_1.8.10": "dev-master"
    },
    "authors": [
        {
            "name": "Aydin Hassan",
            "email": "aydin@wearejh.com"
        }
    ],
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "composer",
            "url": "http://packages.firegento.com"
        },
        {
            "type": "vcs",
            "url": "git@bitbucket.org:jhhello/jh_magento_composer_autoloader.git"
        },
        {
            "type": "vcs",
            "url": "git@bitbucket.org:jhhello/magento_ce_1.8.0.0"
        },

        {
            "type": "vcs",
            "url": "git@bitbucket.org:AydinHassan/test-magento_ce_1.8.1.0.git"

        },
        {
            "type": "vcs",
            "url": "https://github.com/AydinHassan/n98-magerun.git"

        },
        {
            "type": "vcs",
            "url": "https://github.com/AydinHassan/magento-core-installer.git"

        },
        {
            "url": "https://github.com/EcomDev/EcomDev_PHPUnit",
            "type": "git"
        },
        {
            "type": "vcs",
            "url": "git@bitbucket.org:jhhello/jh_magento_boilerplate_ce.git"
        }
    ],
    "extra":{
        "magento-root-dir": "htdocs/",
        "magento-force": true,
        "magento-deploystrategy": "copy",
        "skip-package-deployment": true
    }

}
