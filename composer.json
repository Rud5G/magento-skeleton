{
    "name": "aydinhassan/magento-skeleton",
    "description": "A Magento Skeleton Application",
    "require-dev": {
        "phpunit/phpunit": "3.7.29",
        "phpunit/phpunit-selenium": "1.3.2",
        "phpunit/dbunit": "1.2.3",
        "phpunit/phpunit-story": "1.0.2",
        "ivanchepurnyi/ecomdev_phpunit": "dev-dev",
        "mikey179/vfsStream": "*"
    },
    "require": {
        "composer/composer": "1.0.0-alpha8",
        "magento-hackathon/magento-composer-installer": "1.4.0",
        "magento-hackathon/composer-command-integrator": "1.0",
        "jhhello/magento-composer-autoloader": "1.0.1",
        "quafzi/magento-core-installer": "1.0.0",
        "jhhello/magento-ce-1.8.1.0": "1.0.0"
    },
    "authors": [
        {
            "name": "Aydin Hassan",
            "email": "aydin@wearejh.com"
        }
    ],
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "composer",
            "url": "http://packages.firegento.com"
        },
        {
            "type": "git",
            "url": "git@bitbucket.org:jhhello/magento-composer-autoloader.git"
        },
        {
            "type": "vcs",
            "url": "git@bitbucket.org:jhhello/magento-ce-1.8.1.0.git"
        },
        {
            "url": "https://github.com/EcomDev/EcomDev_PHPUnit",
            "type": "git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/AydinHassan/magento-composer-installer.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/AydinHassan/magento-core-installer.git"
        }
    ],
    "process-timeout": "600",
    "extra":{
        "magento-root-dir": "htdocs/",
        "magento-force": true,
        "skip-package-deployment": true
    }

}
