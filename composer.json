{
    "name": "aydinhassan/magento-skeleton",
    "description": "A Magento Skeleton Application",
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "phpunit/phpunit-selenium": "1.3.2",
        "phpunit/dbunit": "1.2.3",
        "phpunit/phpunit": "3.7.x-dev",
        "phpunit/phpunit-story": "1.0.2",
        "ivanchepurnyi/ecomdev_phpunit": "0.3.1",
        "magento-hackathon/magento-composer-installer": "*"
    },
    "require": {
        "jhhello/magento_composer_autoloader": "master"
    },
    "authors": [
        {
            "name": "Aydin Hassan",
            "email": "aydin@wearejh.com"
        }
    ],
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "composer",
            "url": "http://packages.firegento.com"
        },
        {
            "type": "vcs",
            "url": "git@bitbucket.org:jhhello/jh_magento_composer_autoloader.git"
        }
    ],
    "extra":{
        "magento-root-dir": "../htdocs/"
    }
}
